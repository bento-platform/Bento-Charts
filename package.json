{
  "name": "bento-charts",
  "version": "2.3.0",
  "description": "Charts library for Bento-platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npx tsc",
    "prepublishOnly": "npm run build",
    "lint": "npx eslint src",
    "buildpack": "rm ./packs/*.tgz && tsc && npm pack --pack-destination ./packs"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bento-platform/Bento-Charts.git"
  },
  "keywords": [
    "Bento",
    "Charts"
  ],
  "author": "Sanjeev Lakhwani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bento-platform/Bento-Charts/issues"
  },
  "homepage": "https://github.com/bento-platform/Bento-Charts#readme",
  "peerDependencies": {
    "leaflet": "^1.9.4",
    "react": ">=16.0.0",
    "react-dom": ">=14.0.0",
    "react-leaflet": "^4.2.1",
    "recharts": "^2.4.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@semantic-release/git": "^10.0.1",
    "@types/leaflet": "^1.9.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "@types/react-leaflet": "^3.0.0",
    "@types/recharts": "^1.8.24",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@webpack-cli/serve": "^2.0.5",
    "css-loader": "^6.8.1",
    "eslint": "^8.36.0",
    "eslint-plugin-react": "^7.32.2",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "prettier": "2.7.1",
    "semantic-release": "^20.1.3",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "printWidth": 120
  },
  "dependencies": {
    "d3-interpolate": "^3.0.1"
  }
}
