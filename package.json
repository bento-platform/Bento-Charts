{
  "name": "bento-charts",
  "version": "1.0.8",
  "description": "Charts library for Bento-platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npx tsc",
    "prepublishOnly": "npm run build",
    "lint": "npx eslint src/**"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bento-platform/Bento-Charts.git"
  },
  "keywords": [
    "Bento",
    "Charts"
  ],
  "author": "Sanjeev Lakhwani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bento-platform/Bento-Charts/issues"
  },
  "homepage": "https://github.com/bento-platform/Bento-Charts#readme",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.4.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@semantic-release/git": "^10.0.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "@types/recharts": "^1.8.24",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "prettier": "2.7.1",
    "semantic-release": "^20.1.3",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5"
  }
}
